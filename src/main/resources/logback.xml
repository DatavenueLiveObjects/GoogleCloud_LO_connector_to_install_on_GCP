<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="com.orange.lo.sample.mqtt2dis" level="DEBUG" />  
    <logger name="com.bigdata.dis.sdk" level="ERROR" />
    
    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>